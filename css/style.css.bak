/* Primary menu settings */
#menu ul > li.selected > p:before {
   content:"";
   position:absolute;
   top:0;
   right:0;
   display:block;
   width:31px;
   height:31px;
   background:url(../images/defSprites.png) no-repeat 12px -1960px transparent;
}
